{
  "expo": {
    "name": "demo",
    "slug": "demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://cloud.appwrite.io/v1",
      "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "67ce0c55000f93dbeb99",
      "EXPO_PUBLIC_APPWRITE_DB_ID": "notes-db",
      "EXPO_PUBLIC_APPWRITE_COL_NOTES_ID": "notes",
      "EXPO_PUBLIC_APPWRITE_BUNDLE_ID": "notes-app-ios",
      "EXPO_PUBLIC_APPWRITE_PACKAGE_NAME": "notes-app"
    }
  }
}
